{{partial argument.type}}
<i class="error-tooltip-anchor fa fa-remove" data-toggle="tooltip" title={{ errors }}></i>

{{#if argument.many}}
    <div class="add-remove-row">
        <i {{action "add"}} class="add fa fa-plus-circle"></i>
        <i {{action "remove"}} class="remove fa fa-minus-circle"></i>
    </div>
{{/if}}
